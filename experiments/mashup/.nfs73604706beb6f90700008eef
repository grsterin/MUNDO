#!/bin/bash

DIM=1000
while getopts "n:d:" args; do
    case $args in 
        n) NETWORK=$OPTARG
            ;;
        d) DIM=$OPTARG
            ;;
    esac
done


WORKING_FOLDER=/cluster/tufts/cowenlab/Projects/UNIMUNDO/MUNDO/network_dir

python mashup_embedding.py --working_folder=${WORKING_FOLDER} --network=${NETWORK} --dim=${DIM} 
